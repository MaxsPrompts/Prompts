{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "Datavio",
  "description": "A chrome extension to help you with analysing product pages on Flipkart",
  "background": { "service_worker": "background.bundle.js" },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Datavio competitor research",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": ["tabs", "storage"],
  "content_scripts": [
    {
      "matches": ["https://www.flipkart.com/*"],
      "js": ["contentScript.bundle.js"]
    }
  ]
}
